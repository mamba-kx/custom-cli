{
  "name": "vvue",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "web-norm": "web-norm",
    "prettier": "prettier --write",
    "eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,js,tsx}\" --fix",
    "serve:test": "cross-env VUE_APP_ENV=test vite",
    "serve:temp": "cross-env VUE_APP_ENV=temp vite",
    "serve:prod": "cross-env VUE_APP_ENV=prod vite",
    "build:test": "cross-env VUE_APP_ENV=test && npm run build",
    "build:temp": "cross-env VUE_APP_ENV=temp && npm run build",
    "build:prod": "cross-env VUE_APP_ENV=prod && npm run build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstallmac": "git config core.hooksPath .husky && chmod 700 .husky/*",
    "commit": "git add . && git-cz"
  },
  "dependencies": {
    "@vueuse/core": "^9.5.0",
    "axios": "^1.1.3",
    "cross-env": "^7.0.3",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.23",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.2.0",
    "cz-git": "^1.3.12",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9",
    "web-norm": "^1.0.27",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.2.0",
    "lint-staged": "^12.4.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.9.0",
    "@commitlint/cz-commitlint": "^17.0.3",
    "inquirer": "^8.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "npm run eslint"
    ],
    "*.{js,jsx,ts,tsx,md,html,css,lees,scss,sass}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
