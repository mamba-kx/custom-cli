{
  "name": "vvue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "commit": "git-cz",
    "prettier": "prettier --write",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "serve:test": "cross-env VUE_APP_ENV=test vite",
    "serve:temp": "cross-env VUE_APP_ENV=temp vite",
    "serve:prod": "cross-env VUE_APP_ENV=prod vite",
    "build:test": "cross-env VUE_APP_ENV=test && npm run build",
    "build:temp": "cross-env VUE_APP_ENV=temp && npm run build",
    "build:prod": "cross-env VUE_APP_ENV=prod && npm run build"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@vueuse/core": "^9.5.0",
    "axios": "^1.1.3",
    "cross-env": "^7.0.3",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.23",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  }
}
